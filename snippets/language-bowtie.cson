  'Main':
    'prefix': 'main'
    'body': 'main {\n\t$1\n}'
  'Map':
    'prefix': 'map'
    'body': 'map {\n\t$1\n}'
  'Mark':
    'prefix': 'mark'
    'body': 'mark: "$1"$0'
  'Menu':
    'prefix': 'menu'
    'body': 'menu {\n\t$1\n}'
  'Menu Item':
    'prefix': 'menuitem'
    'body': 'menuitem(type="${1:command}" label="${2:Save}")$0'
  'Meter':
    'prefix': 'meter'
    'body': 'meter(min="${1:200}" max="${2:500}" value="${3:350}")$0'
  'Mail Anchor':
    'prefix': 'mailto'
    'body': 'a(href="mailto:${1:joe@example.com}?subject=${2:feedback}"): "${3:email me}"$0'
  'Meta':
    'prefix': 'meta'
    'body': 'meta(name="${1:name}" content="${2:content}")$0'
  # N
  'Navigation':
    'prefix': 'nav'
    'body': 'nav {\n\t$1\n}'
  'Noscript':
    'prefix': 'noscript'
    'body': 'noscript {\n\t$1\n}'
  # O
  'Object':
    'prefix': 'object'
    'body': 'object(data="${1:http://}" type="${2:mimetype}"): "$3"$0'
  'Ordered List':
    'prefix': 'ol'
    'body': 'ol {\n\t$1\n}'
  'Option Group':
    'prefix': 'optgroup'
    'body': 'optgroup(label="${1:Group 1}") {\n\t$2\n}'
  'Option':
    'prefix': 'opt'
    'body': 'option(${1:value="${2:option}"}): "${3:option}"$0'
  'Output':
    'prefix': 'output'
    'body': 'output(name="${1:result}"): "$2"$0'
  # P
  'Paragraph':
    'prefix': 'p'
    'body': 'p {\n\t$1\n}'
  'Parameter':
    'prefix': 'param'
    'body': 'param(name="${1:foo}" value="${2:bar}")$0'
  'Preformatted Text':
    'prefix': 'pre'
    'body': 'pre {\n\t$1\n}'
  'Progress':
    'prefix': 'progress'
    'body': 'progress(value="${1:50}" max="${2:100}"): "${3:50%}"$0'
  # Q
  'Quote':
    'prefix': 'q'
    'body': 'q(cite="$1") "$2"$0'
  # R
  'Ruby Base':
    'prefix': 'rb'
    'body': 'rb: "$1"$0'
  'Ruby Parenthesis':
    'prefix': 'rp'
    'body': 'rp: "$1"$0'
  'Ruby Pronunciation':
