  'Subscript':
    'prefix': 'sub'
    'body': 'sub: "$1"$0'
  'Summary':
    'prefix': 'summary'
    'body': 'summary: "$1"$0'
  'Superscript':
    'prefix': 'sup'
    'body': 'sup: "$1"$0'
  # T
  'Table':
    'prefix': 'table'
