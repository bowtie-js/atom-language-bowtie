'.source.bow':
  # BOWTIE Specific
  'New mixin':
    'prefix': '$mixin'
    'body': '$${1:MixinName} -> (${2:$parameter}) {\n\t${3}\n}'
  # HTML
  # A
  'Anchor':
    'prefix': 'a'
    'body': 'a(href="${1:#}"): "$2"$0'
  'Abbreviation':
    'prefix': 'abbr'
    'body': 'abbr(title="$1"): "$2"$0'
  'Address':
    'prefix': 'address'
    'body': 'address.${1:class} {\n\t$3\n}'
  'Area':
    'prefix': 'area'
    'body': 'area(${1:shape="${2:default}"} coords="$3" ${4:href="${5:#}"})$0'
  'Article':
    'prefix': 'article'
    'body': 'article.${1:class} {\n\t$2\n}'
  'Aside':
    'prefix': 'aside'
    'body': 'aside.${1:class} {\n\t$2\n}'
  'Audio':
    'prefix': 'audio'
    'body': 'audio(src="$1") {\n\t$2\n}'
  # B
  'Body':
    'prefix': 'body'
    'body': 'body {\n\t$1\n}'
  'Bold':
    'prefix': 'b'
    'body': 'b: "$1" $0'
  'Base':
    'prefix': 'base'
    'body': 'base(ref="${1:#}" target="${2:_blank}")$0'
  'Bi-Directional Isolation':
    'prefix': 'bdi'
    'body': 'bdi(dir="${1:auto}"): "$2"$0'
  'Bi-Directional Override':
    'prefix': 'bdo'
    'body': 'bdo(dir="${1:auto}"): "$2"$0'
  'Blockquote':
    'prefix': 'blockquote'
    'body': 'blockquote(cite="${1:http://}") {\n\t$2\n}'
  'Line Breaker':
    'prefix': 'br'
    'body': 'br'
  'Button':
    'prefix': 'button'
    'body': 'button(type="${1:button}"${2: name="${3:button}"}): "$4"$0'
  # C
  'Canvas':
    'prefix': 'canvas'
    'body': 'canvas#${1:id}(width="${2:300}" height="${3:300}"): "$4"$0'
  'Caption':
    'prefix': 'caption'
    'body': 'caption: "$1"$0'
  'Citation':
    'prefix': 'cite'
    'body': 'cite: "$1"$0'
  'Code':
    'prefix': 'code'
    'body': 'code {\n\t$1\n}$0'
  'Column':
    'prefix': 'col'
    'body': 'col: "$1"$0'
  'Column Group':
    'prefix': 'colgroup'
    'body': 'colgroup: "$1"$0'
  'Content':
    'prefix': 'content'
    'body': 'content(select="$1"): "$2"$0'
  # D
  'HTML â€” 5':
    'prefix': 'doctype'
    'body': '!doctype("html")\n'
  'Data':
    'prefix': 'data'
    'body': 'data(value="$1"): "$2"$0'
  'Data List':
    'prefix': 'datalist'
    'body': 'datalist.${1:class} {\n\t$2\n}'
  'Description':
    'prefix': 'dd'
    'body': 'dd: "$1"$0'
  'Deleted Text':
    'prefix': 'del'
    'body': 'del: "$1"$0'
  'Details':
    'prefix': 'details'
    'body': 'details(${1:open}): "$2"$0'
  'Definition':
    'prefix': 'dfn'
    'body': 'dfn: "$1"$0'
  'Description List':
    'prefix': 'dl'
    'body': 'dl.${1:class} {\n\t$2\n}'
  'Definition Term':
    'prefix': 'dt'
    'body': 'dt: "$1"$0'
  'Div':
    'prefix': 'div'
    'body': 'div.${1:class} {\n\t$2\n}'
  # E
  'Emphasis':
    'prefix': 'em'
    'body': 'em: "$1"$0'
  'Embed':
    'prefix': 'embed'
    'body': 'embed(type="${1:video/quicktime}" src="${2:#}" width="${3:300}" height="${4:300}")$0'
  # F
  'Fieldset':
    'prefix': 'fieldset'
    'body': 'fieldset: "$1"$0'
  'Figure Caption':
    'prefix': 'figcaption'
    'body': 'figcaption: "$1"$0'
  'Figure':
    'prefix': 'figure'
    'body': 'figure: "$1"$0'
  'Footer':
    'prefix': 'footer'
    'body': 'footer: "$1"$0'
  'Form':
    'prefix': 'form'
    'body': 'form.${1:class}(action="${2:index.html}" method="${3:post}") {\n\t$4\n}'
  # G
  # H
  'Heading 1':
    'prefix': 'h1'
    'body': 'h1: "$1"$0'
  'Heading 2':
    'prefix': 'h2'
    'body': 'h2: "$1"$0'
  'Heading 3':
    'prefix': 'h3'
    'body': 'h3: "$1"$0'
  'Heading 4':
    'prefix': 'h4'
    'body': 'h4: "$1"$0'
  'Heading 5':
    'prefix': 'h5'
    'body': 'h5: "$1"$0'
  'Heading 6':
    'prefix': 'h6'
    'body': 'h6: "$1"$0'
  'Head':
    'prefix': 'head'
    'body': 'head {\n\t$1\n}'
  'Header':
    'prefix': 'header'
    'body': 'header {\n\t$1\n}'
  'Horizontal Rule':
    'prefix': 'hr'
    'body': 'hr'
  'HTML':
    'prefix': 'html'
    'body': '!doctype("html")\nhtml {\n\thead {\n\t\tmeta(charset="utf-8")\n\t\ttitle: "$1"\n\t}\n\tbody {\n\t\t$2\n\t}\n}'
  # I
  'Italic':
    'prefix': 'i'
    'body': 'i: "$1"$0'
  'Inline Frame':
    'prefix': 'iframe'
    'body': 'iframe(src="$1" width="$2" height="$3"): "$4"$0'
  'Input':
    'prefix': 'input'
    'body': 'input(type="${1:button}" name="${2:name}" value="$3")$0'
  'Image':
    'prefix': 'img'
    'body': 'img(src="$1" alt="$2")$0'
  'Import (html)':
    'prefix': 'import'
    'body': 'link(rel="import" href="$1")$0'
  'Inserted Text':
    'prefix': 'ins'
    'body': 'ins: "$1"$0'
  # J
  # K
  'Keyboard Input':
    'prefix': 'kbd'
    'body': 'kbd: "$1"$0'
  'Keygen':
    'prefix': 'keygen'
    'body': 'keygen(name="${1:name}" challenge="${2:string}" keytype="${3:RSA}" keyparams="${4:medium}")$0'
  # L
  'Label':
    'prefix': 'label'
    'body': 'label(${1: for="$2"}): "$3"$0'
  'Legend':
    'prefix': 'legend'
    'body': 'legend: "$1"$0'
  'List Item':
    'prefix': 'li'
    'body': 'li: "$1"$0'
  'Link':
    'prefix': 'link'
    'body': 'link(rel="${1:stylesheet}" href="${2:/css/master.css}" media="${3:screen}" title="${4:no title}" charset="${5:utf-8}")$0'
